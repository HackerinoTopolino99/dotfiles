{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#011627",
    "black": "#1d3b53",
    "blue": "#82aaff",
    "bold": "#eeeeee",
    "bright_green": "#21c7a8",
    "bright_white": "#d6deeb",
    "cursor": "#9ca1aa",
    "cursortext": "#080808",
    "cyan": "#7fdbca",
    "fern": "#87d787",
    "foreground": "#bdc1c6",
    "green": "#a1cd5e",
    "purple": "#c792ea",
    "red": "#fc514e",
    "selection": "#b2ceee",
    "selectiontext": "#080808",
    "white": "#a1aab8",
    "yellow": "#e3d180"
  },
  "secondary_prompt": {
    "template": "-> ",
    "foreground": "p:foreground",
    "background": "p:background"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "display_mode": "environment",
            "fetch_version": false,
            "fetch_virtual_env": true
          },
          "template": "\ue235 ",
          "foreground": "p:yellow",
          "background": "p:background",
          "type": "python",
          "style": "plain"
        },
        {
          "options": {
            "macos": "mac"
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{ .Icon }} ",
          "foreground": "#26C6DA",
          "background": "p:background",
          "type": "os",
          "style": "powerline"
        },
        {
          "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }}",
          "foreground": "p:bright_green",
          "background": "p:background",
          "type": "session",
          "style": "powerline"
        },
        {
          "options": {
            "style": "folder"
          },
          "foreground": "p:red",
          "powerline_symbol": "\ue0b0",
          "background": "p:background",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "mapped_branches": {
              "bug/*": "üêõ ",
              "feat/*": "\ud83d\ude80 "
            },
            "source": "cli",
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          },
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "foreground": "p:green",
          "powerline_symbol": "\ue0b0",
          "background": "p:background",
          "type": "git",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "options": {
            "always_enabled": true
          },
          "trailing_diamond": "\ue0b4",
          "template": "<p:cyan>\ue0b0</> \ue23a ",
          "foreground": "p:green",
          "background": "p:background",
          "type": "status",
          "style": "diamond",
          "foreground_templates": [
            "{{ if .Error }}p:red{{ end }}"
          ]
        }
      ]
    }
  ],
  "version": 4,
  "final_space": true
}
